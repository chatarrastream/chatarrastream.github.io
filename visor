    aY.sort((a, b) => a[0] - b[0]);
    var k = 50006 + 752914;
    aY.forEach(e => {
      let v = e[1];
      playbackURL += String.fromCharCode(parseInt(atob(v).replace(/\D/g, '')) - k);
    });

    var p2pConfig = {
      trackerZone: 'us',
      sourceUrl: playbackURL
    };

    P2PEngineHls.tryRegisterServiceWorker(p2pConfig).then(() => {
      var player = new Clappr.Player({
        source: playbackURL,
        parentId: "#player",
        plugins: [SwarmCloudClapprPlugin],
        width: '100%',
        height: '100%',
        autoPlay: false,
        mute: true,
        playsinline: true,
        playback: {
          hlsjsConfig: {
            maxBufferSize: 0,
            maxBufferLength: 12,
            liveSyncDurationCount: 3,
            p2pConfig: p2pConfig
          }
        }
      });
      window.player = player;
    });

    function unmutePlayer() {
      if (window.player) {
        player.setVolume(100);
        player.configure({ mute: false });
        document.getElementById('soundButton').style.display = 'none';
      }
    }
